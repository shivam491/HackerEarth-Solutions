https://www.hackerearth.com/problem/algorithm/problem-3-16/
--------
#include <bits/stdc++.h>
using namespace std;

int main()
{
	long long int t,i,j,k,l,m,n,o,p;
	cin>>t;
	while(t--)
	{
		cin>>n>>k;
		long long int a[k];
		memset(a,0,sizeof(a));
		for(i=0;i<n;i++)
		{
			cin>>j;
			a[j-1]++;
		}
		p=0;
		for(i=0;i<k;i++)
		p=max(p,a[i]);
		
		j=n-p;
		cout<<j<<"\n";
		
	}
}
